{
  "name": "scholarsome",
  "version": "1.0.2",
  "license": "AGPL-3.0-or-later",
  "scripts": {
    "ng": "nx",
    "generate": "npx prisma generate",
    "migrate": "npx prisma migrate deploy",
    "build": "npx nx run-many --target=build --projects=front,api --skip-nx-cache",
    "build:docs": "npx nx build docs --skip-nx-cache",
    "test": "nx test",
    "lint": "nx run-many --all --target=lint --fix",
    "start": "nx serve",
    "start:front": "npx nx serve --host=0.0.0.0",
    "start:api": "npx nx serve api",
    "start:dev": "concurrently -p=\"{name}\" -n=\"Angular,NestJS\" -c=\"green,blue\" \"npm run start:front\" \"npm run start:api\"",
    "serve:node": "npm run migrate && node dist/apps/api/main.js",
    "serve:pm2": "npm run migrate && pm2 start dist/apps/api/main.js --name \"scholarsome\""
  },
