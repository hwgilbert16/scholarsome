  "name": "scholarsome",
  "version": "1.1.0",
  "license": "AGPL-3.0-or-later",
  "scripts": {
    "ng": "nx",
    "generate": "npx prisma generate",
    "migrate": "npx prisma migrate deploy",
    "build": "npx nx run-many --target=build --projects=front,api,docs --skip-nx-cache",
    "test": "npx nx test api",
    "lint": "nx run-many --all --target=lint --fix",
    "start": "nx serve",
    "start:front": "npx nx serve --host=0.0.0.0",
    "start:api": "npx nx serve api",
    "start:docs": "npx nx serve docs",
    "start:dev": "concurrently -p=\"{name}\" -n=\"Angular,NestJS, Docusaurus\" -c=\"red,orange,green\" \"npm run start:front\" \"npm run start:api\" \"npm run start:docs\"",
    "serve:node": "npm run migrate && node dist/apps/api/main.js",
    "serve:pm2": "npm run migrate && pm2 start dist/apps/api/main.js --name \"scholarsome\"",
    "prepare": "husky"
  },
  "private": true,
